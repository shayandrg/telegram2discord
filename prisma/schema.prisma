generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db.db"
}

model App {
  id Int @id @default(autoincrement())
  key String
  value String
}

model discordUploadQuee {
  id Int @id @default(autoincrement())
  fileName String
  fileSize Int
  isUploading Boolean @default(false)
}

model file {
  id Int @id @default(autoincrement())
  fileName String
  fileSize Int
  url String
}